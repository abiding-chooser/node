# Copy to .env before running locally
# cp .env.example .env

# RPC URL for local or remote Ethereum node
RPC_URL=http://localhost:8545

# Network name (mainnet, testnet, custom networks, etc.)
NETWORK=development

# Optional API key for any external services (if required)
API_KEY=

# Logging level for debugging (info | debug | warn)
LOG_LEVEL=info
